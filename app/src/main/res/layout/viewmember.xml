<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:visibility="visible"
        android:contentDescription="id_12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:paddingRight="15dp"
        android:paddingLeft="30dp"
        android:orientation="vertical"
        android:background="@drawable/ic_name_bg">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_Fullname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Manoj Sah"
                    android:contentDescription="Name"
                    android:textStyle="bold"
                    android:textColor="@color/white"
                    android:textFontWeight="900"
                    android:fontFamily="@font/sofia_pro_bold_az"
                    android:textSize="22sp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_employee"
                    android:text="Employee"
                    android:contentDescription="Company"
                    android:textStyle="normal"
                    android:textColor="#CCFFFFFF"
                    android:fontFamily="@font/sofia_pro_light_az"
                    android:textSize="16sp"
                    android:letterSpacing="0.1"/>

                <TextView
                    android:id="@+id/tv_Address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Chirauli, Bhokraha - 1"
                    android:contentDescription="Address"
                    android:textStyle="normal"
                    android:textColor="#CCFFFFFF"
                    android:fontFamily="@font/sofia_pro_light_az"
                    android:textSize="12sp"
                    android:letterSpacing="0.2"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ivCallC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:contentDescription="9801149729"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/iv_Call"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="5dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/ic_call"/>

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="12dp">

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="रू"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="18dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="-16dp"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_profile_databg"/>

                    <TextView
                        android:id="@+id/TV_Price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="28640"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>

                </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="15dp"
                        android:src="@drawable/ic_cylinder_empty"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="-16dp"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_profile_databg"/>

                    <TextView
                        android:id="@+id/tv_Halfcylinder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="20"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>
                </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_cylinder_leak"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="-16dp"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_profile_databg"/>

                    <TextView
                        android:id="@+id/tv_leckcyclinder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="20"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>
                </FrameLayout>


            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="12dp">

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="17dp"
                        android:layout_height="17dp"
                        android:src="@drawable/ic_cylinder_rate"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="-16dp"
                    android:animateLayoutChanges="true">


                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"/>

                    <TextView
                        android:id="@+id/tv_Tag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="1343"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>


                </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="15dp"
                        android:src="@drawable/ic_cylinder_gas"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-16dp"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"/>

                    <TextView
                        android:id="@+id/tv_Burn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="3458"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>


                </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:src="@drawable/ic_empty_shape"
                        android:layout_gravity="center"/>

                    <ImageView
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:src="@drawable/ic_circle_bg"/>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-16dp"
                    android:layout_gravity="center"
                    android:animateLayoutChanges="true">

                    <ImageView
                        android:layout_width="75dp"
                        android:layout_height="35dp"/>

                    <TextView
                        android:id="@+id/tv_Cylinder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="50"
                        android:fontFamily="@font/sofia_pro_regular_az"
                        android:textStyle="normal"
                        android:textSize="14dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center"/>


                </FrameLayout>


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</LinearLayout>